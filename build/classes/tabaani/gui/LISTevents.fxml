<AnchorPane id="AnchorPane" prefHeight="706.0" prefWidth="1212.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tabaani.gui.LISTeventsController">
    <children>
        <ImageView fx:id="logoImg" fitHeight="43.0" fitWidth="142.0" layoutX="1062.0" layoutY="649.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/Tabaani%20light%20bg.png" />
         </image>
      </ImageView>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="&lt; Back" textFill="#000b46">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Label layoutX="193.0" layoutY="28.0" text="List of your events " textFill="#3c1bb2">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <TableView fx:id="tblThemes" layoutX="12.0" layoutY="72.0" prefHeight="561.0" prefWidth="1190.0">
        <columns>
          <TableColumn fx:id="nameE" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="maxE" prefWidth="75.0" text="Max Particiants" />
            <TableColumn fx:id="picE" prefWidth="75.0" text="Picture" />
