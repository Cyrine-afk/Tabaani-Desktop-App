            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </TableView>
      <Button fx:id="btnEdit" layoutX="21.0" layoutY="652.0" mnemonicParsing="false" onAction="#go_editT" prefHeight="34.0" prefWidth="69.0" text="Edit" textFill="#25bf4c">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnDelete" layoutX="99.0" layoutY="652.0" mnemonicParsing="false" onAction="#go_deleteT" prefHeight="37.0" prefWidth="76.0" text="Delete" textFill="#25bf4c">
