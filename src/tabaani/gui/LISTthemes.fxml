<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tabaani.gui.LISTthemesController">
    <children>
        <ImageView fx:id="logoImg" fitHeight="43.0" fitWidth="142.0" layoutX="450.0" layoutY="346.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/Tabaani%20light%20bg.png" />
         </image>
      </ImageView>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="&lt; Back" textFill="#000b46">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Label layoutX="193.0" layoutY="28.0" text="List of your themes " textFill="#3c1bb2">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <TableView fx:id="tblThemes" layoutX="12.0" layoutY="72.0" prefHeight="257.0" prefWidth="578.0">
        <columns>
          <TableColumn fx:id="nameTh" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="picTh" prefWidth="75.0" text="Picture" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </TableView>
      <Button fx:id="btnEdit" layoutX="26.0" layoutY="349.0" mnemonicParsing="false" onAction="#go_editT" prefHeight="34.0" prefWidth="69.0" text="Edit" textFill="#25bf4c">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnDelete" layoutX="104.0" layoutY="349.0" mnemonicParsing="false" onAction="#go_deleteT" prefHeight="37.0" prefWidth="76.0" text="Delete" textFill="#25bf4c">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
