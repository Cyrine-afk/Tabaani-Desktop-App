<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tabaani.gui.ThemesMenuController">
   <children>
      <ImageView fx:id="logoImg" fitHeight="67.0" fitWidth="138.0" layoutX="448.0" layoutY="342.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/Tabaani%20light%20bg.png" />
         </image>
      </ImageView>
      <Label layoutX="142.0" layoutY="80.0" text="Manage your themes here !" textFill="#074b9e">
         <font>
            <Font name="System Bold" size="29.0" />
         </font>
      </Label>
      <Button fx:id="btnAddT" layoutX="206.0" layoutY="166.0" mnemonicParsing="false" onAction="#go_addT" prefHeight="46.0" prefWidth="188.0" text="Add Theme" textFill="#25bf4c">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
      <Button fx:id="btnListT" layoutX="206.0" layoutY="232.0" mnemonicParsing="false" onAction="#go_listT" prefHeight="46.0" prefWidth="188.0" text="List Themes" textFill="#25bf4c">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="&lt; Back" textFill="#000b46">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
